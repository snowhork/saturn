        "title": "OAuthToken"
      },
      "Storage": {
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "type": {
            "type": "string",
            "enum": [
              "google_drive",
              "local"
            ],
            "title": "Type"
          }
        },
        "type": "object",
        "required": [
          "name",
          "type"
        ],
        "title": "Storage"
      },
      "TransferRequest": {
        "properties": {
          "src_name": {
            "type": "string",
            "title": "Src Name"
          },
          "src_ids": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Src Ids"
          },
          "dst_name": {
            "type": "string",
            "title": "Dst Name"
          },
          "dst_id": {
            "type": "string",
            "title": "Dst Id"
          },
          "src_credentials": {
            "$ref": "#/components/schemas/CredentialsRequest"
          },
          "dst_credentials": {
