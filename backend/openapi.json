        "type": "object",
        "required": [
          "name",
          "id",
          "is_dir"
        ],
        "title": "Item"
      },
      "OAuthToken": {
        "properties": {
          "access_token": {
            "type": "string",
            "title": "Access Token"
          },
          "token_type": {
            "type": "string",
            "title": "Token Type"
          },
          "expires_in": {
            "type": "integer",
            "title": "Expires In"
          },
          "refresh_token": {
            "type": "string",
            "title": "Refresh Token"
          },
          "scope": {
            "items": {
              "type": "string"
            },
